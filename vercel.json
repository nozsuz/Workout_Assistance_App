{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "icon.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gemini",
      "dest": "server.js"
    },
    {
      "src": "/icon.png",
      "dest": "icon.png"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}